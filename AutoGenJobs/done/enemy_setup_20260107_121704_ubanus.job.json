{
  "schemaVersion": 1,
  "jobId": "enemy_setup_20260107_121704_ubanus",
  "createdAtUtc": "2026-01-07T12:17:04Z",
  "projectWriteRoot": "Assets/AutoGen",
  "requiresTypes": [
    "EnemyConfig",
    "Enemy"
  ],
  "commands": [
    {
      "cmd": "CreateGameObject",
      "args": {
        "name": "Enemies",
        "ensure": true
      },
      "out": {
        "go": "$enemiesRoot"
      }
    },
    {
      "cmd": "CreateScriptableObject",
      "args": {
        "type": "EnemyConfig",
        "assetPath": "Assets/AutoGen/Configs/EnemyConfig.asset",
        "overwrite": true,
        "init": {
          "HP": 100,
          "Speed": 5
        }
      },
      "out": {
        "asset": "$config"
      }
    },
    {
      "cmd": "CreateOrEditPrefab",
      "args": {
        "prefabPath": "Assets/AutoGen/Prefabs/Enemy.prefab",
        "rootName": "Enemy",
        "edits": [
          {
            "cmd": "AddComponent",
            "args": {
              "target": {
                "ref": "$prefabRoot"
              },
              "type": "Enemy",
              "ifMissing": true
            },
            "out": {
              "component": "$enemyComponent"
            }
          },
          {
            "cmd": "SetSerializedProperty",
            "args": {
              "target": {
                "ref": "$enemyComponent"
              },
              "propertyPath": "config",
              "value": {
                "ref": "$config"
              }
            }
          }
        ]
      },
      "out": {
        "prefab": "$prefab"
      }
    },
    {
      "cmd": "InstantiatePrefabInScene",
      "args": {
        "prefabPath": "Assets/AutoGen/Prefabs/Enemy.prefab",
        "nameOverride": "Enemy",
        "parentPath": "Enemies",
        "position": [
          0,
          0,
          0
        ],
        "ensure": true,
        "ensureTag": "enemy_instance"
      },
      "out": {
        "instance": "$instance"
      }
    },
    {
      "cmd": "SaveAssets",
      "args": {
        "refresh": true
      }
    }
  ]
}